# LiveKit credentials (same as Spring Boot backend)
LIVEKIT_URL=wss://your-livekit-server.livekit.cloud
LIVEKIT_API_KEY=your_livekit_api_key
LIVEKIT_API_SECRET=your_livekit_api_secret

# Groq API key (same as Spring Boot backend)
GROQ_API_KEY=your_groq_api_key

# Optional overrides
GROQ_MODEL=llama-3.3-70b-versatile
GROQ_WHISPER_MODEL=whisper-large-v3-turbo

# Edge-TTS voice â€” see available voices: edge-tts --list-voices
# Good options: en-US-JennyNeural, en-US-GuyNeural, en-GB-SoniaNeural
TTS_VOICE=en-US-JennyNeural
